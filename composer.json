{
  "name": "sunvalley-technologies/react-php-file-logger",
  "description": "A simple PSR file logger for react php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "SunValley Technologies",
      "email": "info@sunvalley.com.es"
    }
  ],
  "require": {
    "php": "~7.2",
    "psr/log": "^1.1",
    "monolog/monolog": "^2 || ^1.23",
    "react/event-loop": "^1.0 || ^0.5 || ^0.4 || ^0.3.5",
    "react/promise": "^2.3 || ^1.2.1",
    "react/promise-stream": "^1.1",
    "react/stream": "^1.0 || ^0.7.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0|~7.0"
  },
  "autoload": {
    "psr-4": {
      "SunValley\\LoopUtil\\FileLogger\\": "src/",
      "SunValley\\LoopUtil\\FileLogger\\Tests\\": "tests/"
    }
  }
}
